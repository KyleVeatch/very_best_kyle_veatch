<div class="page-header">
  <h1>
    Very best dishes bookmarks
    <a href="/very_best_dishes_bookmarks/new" class="btn btn-lg btn-success">New Very best dishes bookmark</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Dish</th>
        <th>Cuisine</th>
        <th>Restaurant</th>
        <th>Actions</th>
      </tr>

      <% @very_best_dishes_bookmarks.each do |very_best_dishes_bookmark| %>
      <tr>
        <td><%= very_best_dishes_bookmark.dish.name %></td>
        <td><%= very_best_dishes_bookmark.cuisine.name %></td>
        <td><%= very_best_dishes_bookmark.restaurant.name %></td>
        <td>
          <a href="/very_best_dishes_bookmarks/<%= very_best_dishes_bookmark.id %>" class="btn btn-primary">Show</a>
          <a href="/very_best_dishes_bookmarks/<%= very_best_dishes_bookmark.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_very_best_dishes_bookmark/<%= very_best_dishes_bookmark.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
